{
"manifest_version": 2,
    "name": "CatFacts On Twitter",
    "description": "Makes Cats great again. Replace the tweets of a twitter user with cat facts",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "options_ui": {
        // Required.
        "page": "options.html",
        // Recommended.
        "chrome_style": true
    },
    "permissions": [
    "storage"
    ],
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" 
    }
}